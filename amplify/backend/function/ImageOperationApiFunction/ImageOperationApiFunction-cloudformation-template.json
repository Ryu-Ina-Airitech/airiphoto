{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "{\"createdOn\":\"Windows\",\"createdBy\":\"Amplify\",\"createdWith\":\"7.6.4\",\"stackType\":\"function-Lambda\",\"metadata\":{}}",
  "Parameters": {
    "CloudWatchRule": {
      "Type": "String",
      "Default": "NONE",
      "Description": " Schedule Expression"
    },
    "deploymentBucketName": {
      "Type": "String"
    },
    "env": {
      "Type": "String"
    },
    "s3Key": {
      "Type": "String"
    },
    "storageUserStorageName": {
      "Type": "String",
      "Default": "storageUserStorageName"
    },
    "storageUserStorageArn": {
      "Type": "String",
      "Default": "storageUserStorageArn"
    },
    "storageUserStorageStreamArn": {
      "Type": "String",
      "Default": "storageUserStorageStreamArn"
    },
    "storageImageStorageName": {
      "Type": "String",
      "Default": "storageImageStorageName"
    },
    "storageImageStorageArn": {
      "Type": "String",
      "Default": "storageImageStorageArn"
    },
    "storageImageStorageStreamArn": {
      "Type": "String",
      "Default": "storageImageStorageStreamArn"
    },
    "storageImageInfoTagsStorageName": {
      "Type": "String",
      "Default": "storageImageInfoTagsStorageName"
    },
    "storageImageInfoTagsStorageArn": {
      "Type": "String",
      "Default": "storageImageInfoTagsStorageArn"
    },
    "storageImageInfoTagsStorageStreamArn": {
      "Type": "String",
      "Default": "storageImageInfoTagsStorageStreamArn"
    },
    "storageTagsStorageName": {
      "Type": "String",
      "Default": "storageTagsStorageName"
    },
    "storageTagsStorageArn": {
      "Type": "String",
      "Default": "storageTagsStorageArn"
    },
    "storageTagsStorageStreamArn": {
      "Type": "String",
      "Default": "storageTagsStorageStreamArn"
    }
  },
  "Conditions": {
    "ShouldNotCreateEnvResources": {
      "Fn::Equals": [
        {
          "Ref": "env"
        },
        "NONE"
      ]
    }
  },
  "Resources": {
    "LambdaFunction": {
      "Type": "AWS::Lambda::Function",
      "Metadata": {
        "aws:asset:path": "./src",
        "aws:asset:property": "Code"
      },
      "Properties": {
        "Code": {
          "S3Bucket": {
            "Ref": "deploymentBucketName"
          },
          "S3Key": {
            "Ref": "s3Key"
          }
        },
        "Handler": "index.handler",
        "FunctionName": {
          "Fn::If": [
            "ShouldNotCreateEnvResources",
            "ImageOperationApiFunction",
            {
              "Fn::Join": [
                "",
                [
                  "ImageOperationApiFunction",
                  "-",
                  {
                    "Ref": "env"
                  }
                ]
              ]
            }
          ]
        },
        "Environment": {
          "Variables": {
            "ENV": {
              "Ref": "env"
            },
            "REGION": {
              "Ref": "AWS::Region"
            },
            "STORAGE_USERSTORAGE_NAME": {
              "Ref": "storageUserStorageName"
            },
            "STORAGE_USERSTORAGE_ARN": {
              "Ref": "storageUserStorageArn"
            },
            "STORAGE_USERSTORAGE_STREAMARN": {
              "Ref": "storageUserStorageStreamArn"
            },
            "STORAGE_IMAGESTORAGE_NAME": {
              "Ref": "storageImageStorageName"
            },
            "STORAGE_IMAGESTORAGE_ARN": {
              "Ref": "storageImageStorageArn"
            },
            "STORAGE_IMAGESTORAGE_STREAMARN": {
              "Ref": "storageImageStorageStreamArn"
            },
            "STORAGE_IMAGEINFOTAGSSTORAGE_NAME": {
              "Ref": "storageImageInfoTagsStorageName"
            },
            "STORAGE_IMAGEINFOTAGSSTORAGE_ARN": {
              "Ref": "storageImageInfoTagsStorageArn"
            },
            "STORAGE_IMAGEINFOTAGSSTORAGE_STREAMARN": {
              "Ref": "storageImageInfoTagsStorageStreamArn"
            },
            "STORAGE_TAGSSTORAGE_NAME": {
              "Ref": "storageTagsStorageName"
            },
            "STORAGE_TAGSSTORAGE_ARN": {
              "Ref": "storageTagsStorageArn"
            },
            "STORAGE_TAGSSTORAGE_STREAMARN": {
              "Ref": "storageTagsStorageStreamArn"
            }
          }
        },
        "Role": {
          "Fn::GetAtt": [
            "LambdaExecutionRole",
            "Arn"
          ]
        },
        "Runtime": "python3.8",
        "Layers": [],
        "Timeout": 25
      }
    },
    "LambdaExecutionRole": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "RoleName": {
          "Fn::If": [
            "ShouldNotCreateEnvResources",
            "airiphotoLambdaRole87d76873",
            {
              "Fn::Join": [
                "",
                [
                  "airiphotoLambdaRole87d76873",
                  "-",
                  {
                    "Ref": "env"
                  }
                ]
              ]
            }
          ]
        },
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Principal": {
                "Service": [
                  "lambda.amazonaws.com"
                ]
              },
              "Action": [
                "sts:AssumeRole"
              ]
            }
          ]
        }
      }
    },
    "lambdaexecutionpolicy": {
      "DependsOn": [
        "LambdaExecutionRole"
      ],
      "Type": "AWS::IAM::Policy",
      "Properties": {
        "PolicyName": "lambda-execution-policy",
        "Roles": [
          {
            "Ref": "LambdaExecutionRole"
          }
        ],
        "PolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Action": [
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents"
              ],
              "Resource": {
                "Fn::Sub": [
                  "arn:aws:logs:${region}:${account}:log-group:/aws/lambda/${lambda}:log-stream:*",
                  {
                    "region": {
                      "Ref": "AWS::Region"
                    },
                    "account": {
                      "Ref": "AWS::AccountId"
                    },
                    "lambda": {
                      "Ref": "LambdaFunction"
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    },
    "AmplifyResourcesPolicy": {
      "DependsOn": [
        "LambdaExecutionRole"
      ],
      "Type": "AWS::IAM::Policy",
      "Properties": {
        "PolicyName": "amplify-lambda-execution-policy",
        "Roles": [
          {
            "Ref": "LambdaExecutionRole"
          }
        ],
        "PolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Action": [
                "dynamodb:Put*",
                "dynamodb:Create*",
                "dynamodb:BatchWriteItem",
                "dynamodb:Get*",
                "dynamodb:BatchGetItem",
                "dynamodb:List*",
                "dynamodb:Describe*",
                "dynamodb:Scan",
                "dynamodb:Query",
                "dynamodb:Update*",
                "dynamodb:RestoreTable*",
                "dynamodb:Delete*"
              ],
              "Resource": [
                {
                  "Ref": "storageUserStorageArn"
                },
                {
                  "Fn::Join": [
                    "/",
                    [
                      {
                        "Ref": "storageUserStorageArn"
                      },
                      "index/*"
                    ]
                  ]
                }
              ]
            },
            {
              "Effect": "Allow",
              "Action": [
                "dynamodb:Put*",
                "dynamodb:Create*",
                "dynamodb:BatchWriteItem",
                "dynamodb:Get*",
                "dynamodb:BatchGetItem",
                "dynamodb:List*",
                "dynamodb:Describe*",
                "dynamodb:Scan",
                "dynamodb:Query",
                "dynamodb:Update*",
                "dynamodb:RestoreTable*",
                "dynamodb:Delete*"
              ],
              "Resource": [
                {
                  "Ref": "storageImageStorageArn"
                },
                {
                  "Fn::Join": [
                    "/",
                    [
                      {
                        "Ref": "storageImageStorageArn"
                      },
                      "index/*"
                    ]
                  ]
                }
              ]
            },
            {
              "Effect": "Allow",
              "Action": [
                "dynamodb:Put*",
                "dynamodb:Create*",
                "dynamodb:BatchWriteItem",
                "dynamodb:Get*",
                "dynamodb:BatchGetItem",
                "dynamodb:List*",
                "dynamodb:Describe*",
                "dynamodb:Scan",
                "dynamodb:Query",
                "dynamodb:Update*",
                "dynamodb:RestoreTable*",
                "dynamodb:Delete*"
              ],
              "Resource": [
                {
                  "Ref": "storageImageInfoTagsStorageArn"
                },
                {
                  "Fn::Join": [
                    "/",
                    [
                      {
                        "Ref": "storageImageInfoTagsStorageArn"
                      },
                      "index/*"
                    ]
                  ]
                }
              ]
            },
            {
              "Effect": "Allow",
              "Action": [
                "dynamodb:Put*",
                "dynamodb:Create*",
                "dynamodb:BatchWriteItem",
                "dynamodb:Get*",
                "dynamodb:BatchGetItem",
                "dynamodb:List*",
                "dynamodb:Describe*",
                "dynamodb:Scan",
                "dynamodb:Query",
                "dynamodb:Update*",
                "dynamodb:RestoreTable*",
                "dynamodb:Delete*"
              ],
              "Resource": [
                {
                  "Ref": "storageTagsStorageArn"
                },
                {
                  "Fn::Join": [
                    "/",
                    [
                      {
                        "Ref": "storageTagsStorageArn"
                      },
                      "index/*"
                    ]
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  },
  "Outputs": {
    "Name": {
      "Value": {
        "Ref": "LambdaFunction"
      }
    },
    "Arn": {
      "Value": {
        "Fn::GetAtt": [
          "LambdaFunction",
          "Arn"
        ]
      }
    },
    "Region": {
      "Value": {
        "Ref": "AWS::Region"
      }
    },
    "LambdaExecutionRole": {
      "Value": {
        "Ref": "LambdaExecutionRole"
      }
    }
